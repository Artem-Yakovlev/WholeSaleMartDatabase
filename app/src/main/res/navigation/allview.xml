<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/allview"
    app:startDestination="@id/viewFragment">

    <fragment
        android:id="@+id/viewFragment"
        android:name="com.example.wholesalemartdatabase.ui.view.ViewFragment"
        android:label="fragment_view"
        tools:layout="@layout/fragment_view" >
        <action
            android:id="@+id/action_viewFragment_to_createItemFragment"
            app:destination="@id/createItemFragment" />
    </fragment>
    <fragment
        android:id="@+id/createItemFragment"
        android:name="com.example.wholesalemartdatabase.ui.view.CreateItemFragment"
        android:label="CreateItemFragment" />
</navigation>